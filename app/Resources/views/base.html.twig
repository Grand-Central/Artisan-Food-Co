<!DOCTYPE html>
<html {{ sonata_seo_html_attributes() }}>
    <head {{ sonata_seo_head_attributes() }}>
        {{ sonata_seo_metadatas() }}
        <meta property="og:url" content="{{app.request.uri}}"/>
        <meta name="viewport" content="user-scalable=no, width=device-width, initial-scale=1, maximum-scale=1">
        <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
        <link rel="icon" type="image/png" href="/favicon-32x32.png" sizes="32x32">
        <link rel="icon" type="image/png" href="/favicon-16x16.png" sizes="16x16">
        <link rel="manifest" href="/manifest.json">
        <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#ffffff">
        <meta name="apple-mobile-web-app-title" content="Made For Drink">
        <meta name="application-name" content="Made For Drink">
        <meta name="theme-color" content="#ffffff">
        {{ sonata_seo_title() }}
        <link href="https://fonts.googleapis.com/css?family=Roboto+Condensed" rel="stylesheet">
        <link href="{{ asset('/css/screen.css') }}" rel="stylesheet" media="screen">
        {% block stylesheets %}{% endblock %}
        <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.miÂ§n.js"></script>
        <![endif]-->
    </head>
    <body>
        {% block body %}{% endblock %}
        <script src="{{ asset('/js/min/javascript.min.js') }}"></script>
        <script src="{{ asset('bundles/applicationcontactform/js/contact-form.js') }}"></script>
        {% block javascript %}{% endblock %}
        {% if app.environment == 'prod' %}
            <script>
                (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
                })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
                ga('create', 'UA-50717835-2', 'auto');
                ga('send', 'pageview');
            </script>
        {% endif %}
    </body>
</html>
